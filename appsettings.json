{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "PredefinedTags": {
    "Tags": [
      {
        "name": "appUrl",
        "value": "https://domain.com"
      }
    ]
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Sentry.Serilog" ],
    "Enrich": [ "FromLogContext" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": {
      "0": {
        "Name": "Console"
      },
      "1": {
        "Name": "File",
        "Args": {
          "path": "log-.txt",
          "rollingInterval": "Day",
          "flushToDiskInterval": "00:00:10"
        }
      },
      "2": {
        "Name": "Sentry",
        "Args": {
          "Dsn": "",
          "MaxRequestBodySize": "Always",
          "AttachStackTrace": true,
          "DiagnosticLevel": "Warning",
          "MinimumBreadcrumbLevel": "Warning",
          "MinimumEventLevel": "Warning"
        }
      }
    }
  }
}
